1741 data.data.categories.forEach((value, index) => {
1742 let url = "https://รอระบบ domain เสร็จ.com/user/product/search?" + new URLSearchParams({category:value.name_cat});
1743 let clonedCategories = boxCateDetail.children[0].cloneNode(true);
1744 clonedCategories.setAttribute("data-bs-title",value.name_cat);
1745 clonedCategories.querySelector("a").setAttribute("href", url);
1746 clonedCategories.querySelector("img").setAttribute("alt", value.name_cat);
1747 clonedCategories.querySelector("img").setAttribute("src", value.icon_img+"?v2.0");
1748 clonedCategories.querySelector("p.icon-text").innerHTML = value.name_cat;
1749 clonedCategories.classList.remove("d-none");
1750 boxCateDetails.classList.remove('d-none');
1751 boxCateDetail.appendChild(clonedCategories);
1752 });
1753 resolve();
1754 }else{
1755 document.querySelector('#boxCateDetail').style = 'justify-content:center';
1756 document.querySelector('#boxCateDetail').innerHTML = "<div class='text-center'><img class='nodata-icon'
src='"+imageNotdata+"'></div>";
1757 resolve();
1758 }
1759 }else if(type === "best_categories"){
1760 if(data.data.categories.length > 0){
1761 data.data.categories.forEach((value, index) => {
1762 let url = "https://รอระบบ domain เสร็จ.com/user/product/search?" + new URLSearchParams({category:value.name_cat});
1763 let clonedBestCategories = document.querySelector('.cate-hit-row .cate-hit').children[0].cloneNode(true);
1764 clonedBestCategories.querySelector("a").setAttribute("href", url);
1765 clonedBestCategories.querySelector(".img-category").setAttribute("alt", value.name_cat);
1766 clonedBestCategories.querySelector('.img-category').setAttribute("src", value.icon_img+"?v2.0");
1767 clonedBestCategories.querySelector('.cate-name').innerHTML = value.name_cat;
1768 document.querySelector('.cate-hit-row .cate-hit').appendChild(clonedBestCategories);
1769 clonedBestCategories.classList.remove('d-none');
1770 document.querySelector('.show-category-best').classList.remove('d-none');
1771 });
1772 resolve();
1773 }else{
1774 document.querySelector('.show-category-best').style = 'justify-content:center';
1775 document.querySelector('.show-category-best').innerHTML = "<div class='text-center'><img class='nodata-icon'
src='"+imageNotdata+"'></div>";
1776 resolve();
1777 }
1778 }
1779 spinnerLoading(arrayLandningPage['categories'][type]['spinner'],"close");
1780 }else{
1781 spinnerLoading(arrayLandningPage['categories'][type]['spinner'],"close");
1782 document.querySelector('#'+arrayLandningPage['categories'][type]['name']).remove();
1783 resolve();
1784 }
1785 }else{
1786 if(type === "categories"){
1787 document.querySelector('#boxCateDetail').style = 'justify-content:center';
1788 document.querySelector('#boxCateDetail').innerHTML = "<div class='text-center'><img class='nodata-icon'
src='"+imageNotdata+"'></div>";
1789 boxCateDetails.classList.remove('d-none');
1790 resolve();
1791 }else if(type === "best_categories"){
1792 document.querySelector('.hide-category .show-category-best').style = 'justify-content:center';
1793 document.querySelector('.hide-category .show-category-best').innerHTML = "<div class='text-center'><img
class='nodata-icon' src='"+imageNotdata+"'></div>";
1794 document.querySelector('.hide-category .show-category-best').classList.remove('d-none');
