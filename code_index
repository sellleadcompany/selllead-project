1540 const tooltipList = document.querySelectorAll('[data-bs-toggle="tooltip"]');
1541 const tooltips = [...tooltipList].map(el => new bootstrap.Tooltip(el));
1542
1543 updateTooltip = (element, title) => {
1544 const tooltipInstance = bootstrap.Tooltip.getInstance(element);
1545
1546 if (!tooltipInstance) {
1547 new bootstrap.Tooltip(element);
1548 }
1549
1550 const updatedInstance = bootstrap.Tooltip.getInstance(element);
1551
1552 if (updatedInstance) {
1553 updatedInstance._config.title = title;
1554 updatedInstance.update();
1555 }
1556 };
