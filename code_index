1460 arrow.classList.add('fa-chevron-down');
1461 arrow.classList.remove('fa-chevron-up');
1462 } else {
1463 arrow.classList.remove('fa-chevron-down');
1464 arrow.classList.add('fa-chevron-up');
1465 }
1466 });
1467 statusResizeMobile = true;
1468 }
1469 window.removeEventListener('scroll', handleScroll);
1470 }
