1804 shops = async ()=>{
1805 return new Promise(resolve => {
1806 spinnerLoading("show-spinner-shop","open");
1807 fetch("https://รอระบบ domain เสร็จ.com/user/landingPage?"+ new URLSearchParams({type: 'shop',path: 'shop',}) ,)
1808 .then(response => {
1809 if (!response.ok) {
1810 throw new Error('HTTP error! Status: '+response.status+'');
1811 }
1812 return response.json();
1813 })
1814 .then(data => {
1815 if(data['status']){
1816 if(data.data.checkStatus === "on"){
1817 if(data.data.shop.length > 0){
1818 data.data.shop.forEach((value, index) => {
1819 let clonedShop = boxShowShop.children[0].cloneNode(true);
1820 let data = {'seller_member_code':value.member_code,'name':value.seller_name,'affiliate':""}
1821 let urlShop = "https://รอระบบ domain เสร็จ.com/user/shop?"+ new URLSearchParams(data);
1822 clonedShop.querySelector(".a-shop-view").setAttribute("href",urlShop);
1823 clonedShop.querySelector('.img-back').setAttribute('style', 'background-image: url('+value.profile_img+')');
1824 clonedShop.querySelector('.shop-name').innerHTML = (value.seller_name != null)?value.seller_name:"
ไม่ระบุ";
1825 clonedShop.querySelector('.shop-detail').innerHTML = (value.detail != null)?value.detail:"-";
1826 clonedShop.classList.remove("d-none");
1827 boxShowShop.appendChild(clonedShop);
1828 });
1829 boxShowShop.querySelector(".d-none").remove();
1830 resolve();
1831 }else{
1832 document.querySelector('#boxShowShop').style = 'justify-content:center';
1833 document.querySelector('#boxShowShop').innerHTML =
"ไม่มีข้อมูล";
1834 resolve();
1835 }
1836 spinnerLoading("show-spinner-shop","close");
1837 }else{
1838 spinnerLoading("show-spinner-shop","close");
1839 document.querySelector('.hide-show-box').remove();
1840 resolve();
1841 }
1842 resolve();
1843 }else{
1844 spinnerLoading("show-spinner-shop","close");
1845 boxShowShop.style = 'justify-content:center';
1846 boxShowShop.innerHTML = "<div class='text-center'><img class='nodata-icon' src='"+imageNotdata+"'></div>";
1847 boxShowShop.classList.add('text-center');
1848 resolve();
1849 }
1850 })
1851 arrayLandningPage['status']['statusShops'] = true;
1852 });
1853 };
