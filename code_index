1494 cartShow = () => {
1495 const box_cart_badge = document.querySelector('.box-cart-badge');
1496 if(box_cart_badge){
1497 const data = {'type':'qty'};
1498 fetch("https://รอระบบ domain เสร็จ.com/cart/detail", {
1499 method: 'POST',
1500 headers: {'X-CSRF-TOKEN':_token,'Content-Type': 'application/json'},
1501 body: JSON.stringify(data),
1502 }).then(response => {
1503 if(!response.ok) {
1504 return Promise.reject(response);
1505 }
1506 return response.json();
1507 }).then(result => {
1508 if(result.status == "success" && result.data.qty){
1509 if(box_cart_badge.classList.contains('d-none')){
1510 box_cart_badge.classList.remove('d-none');
1511 }
1512 box_cart_badge.querySelector('span').innerHTML = result.data.qty;
1513
1514 if(screeWidth <= 768){
1515 let qtyCart = result.data.qty;
1516 if(qtyCart > 9){
1517 qtyCart = "9+";
1518 }
1519 BoxCartMB.innerHTML = qtyCart;
1520 BoxCartMB.classList.remove('d-none');
1521 }
1522 }
1523 }).catch((error) => {
1524 if(!box_cart_badge.classList.contains('d-none')){
1525 box_cart_badge.classList.add('d-none');
1526 }
1527 });
1528 }
1529 };
