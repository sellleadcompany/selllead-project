1405 const functionLang = () =>{
1406 if (window.innerWidth >= 768) {
1407 document.querySelector('.show-lang-mobile').classList.add('d-none');
1408 document.querySelector('.show-lang-pc').classList.remove('d-none');
1409 document.querySelector('.nav-head-top').classList.remove('d-none');
1410 document.getElementById('slide-head-menu').classList.add('slide-head-menu');
1411 document.querySelectorAll('.show-lang-mobile.manual-mobile').forEach(element => {
1412 element.classList.add('d-none');
1413 });
1414 const dropdownBtn = document.querySelectorAll('.dropdown-btn,.dropdown-item-head');
1415 const dropdownMenu = document.querySelector('.show-lang-pc .dropdown-menu');
1416 if(!statusResizePc){
1417 dropdownBtn.forEach(element => {
1418 const arrow = element.querySelector('.dropdown-btn .arrow,.show-lang-pc .arrow');
1419 element.addEventListener('click', function(event) {
1420 event.stopPropagation();
1421 dropdownMenu.classList.toggle('show');
1422 if (dropdownMenu.classList.contains('show')) {
1423
document.querySelector('.show-lang-pc').style="left:"+(event.target.closest('.dropdown-btn').getBoundingCli
entRect().right+
window.scrollX)+"px;"+"top:"+(event.target.closest('.dropdown-btn').getBoundingClientRect().top+
window.scrollY)+"px"
1424 arrow.classList.add('fa-chevron-down');
1425 arrow.classList.remove('fa-chevron-up');
1426 } else {
1427 document.querySelector('.show-lang-pc').removeAttribute('style');
1428 arrow.classList.remove('fa-chevron-down');
1429 arrow.classList.add('fa-chevron-up');
1430 }
1431 });
1432 });
1433
1434 document.addEventListener('click', function(event) {
1435 if (dropdownMenu.classList.contains('show')) {
1436 document.querySelector('.show-lang-pc').removeAttribute('style');
1437 dropdownMenu.classList.remove('show');
1438 }
1439 });
1440 statusResizePc = true;
1441 }
1442 if (window.scrollY > 35) {
1443 dropdownMenu.classList.remove('show');
1444 document.getElementById('slide-head-menu').classList.remove('slide-head-menu');
1445 }
1446 window.addEventListener('scroll', handleScroll);
1447 }else{
1448 document.querySelector('.show-lang-pc').classList.add('d-none');
1449 document.querySelector('.show-lang-mobile').classList.remove('d-none');
1450 document.getElementById('slide-head-menu').classList.remove('slide-head-menu');
1451 document.querySelector('.nav-head-top').classList.add('d-none');
