2743 const tooltipList = document.querySelectorAll('[data-bs-toggle="tooltip"]');
2744 const tooltips = [...tooltipList].map(el => new bootstrap.Tooltip(el));
2745
2746 updateTooltip = (element, title) => {
2747 const tooltipInstance = bootstrap.Tooltip.getInstance(element);
2748
2749 if (!tooltipInstance) {
2750 new bootstrap.Tooltip(element);
2751 }
2752
2753 const updatedInstance = bootstrap.Tooltip.getInstance(element);
2754
2755 if (updatedInstance) {
2756 updatedInstance._config.title = title;
2757 updatedInstance.update();
2758 }
2759 };
