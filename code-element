2697 cartShow = () => {
2698 const box_cart_badge = document.querySelector('.box-cart-badge');
2699 if(box_cart_badge){
2700 const data = {'type':'qty'};
2701 fetch("https://รอระบบ domain เสร็จ.com/cart/detail", {
2702 method: 'POST',
2703 headers: {'X-CSRF-TOKEN':_token,'Content-Type': 'application/json'},
2704 body: JSON.stringify(data),
2705 }).then(response => {
2706 if(!response.ok) {
2707 return Promise.reject(response);
2708 }
2709 return response.json();
2710 }).then(result => {
2711 if(result.status == "success" && result.data.qty){
2712 if(box_cart_badge.classList.contains('d-none')){
2713 box_cart_badge.classList.remove('d-none');
2714 }
2715 box_cart_badge.querySelector('span').innerHTML = result.data.qty;
2716
2717 if(screeWidth <= 768){
2718 let qtyCart = result.data.qty;
2719 if(qtyCart > 9){
2720 qtyCart = "9+";
2721 }
2722 BoxCartMB.innerHTML = qtyCart;
2723 BoxCartMB.classList.remove('d-none');
2724 }
2725 }
2726 }).catch((error) => {
2727 if(!box_cart_badge.classList.contains('d-none')){
2728 box_cart_badge.classList.add('d-none');
2729 }
2730 });
2731 }
2732 };
