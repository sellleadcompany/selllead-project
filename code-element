2608 const functionLang = () =>{
2609 if (window.innerWidth >= 768) {
2610 document.querySelector('.show-lang-mobile').classList.add('d-none');
2611 document.querySelector('.show-lang-pc').classList.remove('d-none');
2612 document.querySelector('.nav-head-top').classList.remove('d-none');
2613 document.getElementById('slide-head-menu').classList.add('slide-head-menu');
