2618 const dropdownMenu = document.querySelector('.show-lang-pc .dropdown-menu');
2619 if(!statusResizePc){
2620 dropdownBtn.forEach(element => {
2621 const arrow = element.querySelector('.dropdown-btn .arrow,.show-lang-pc .arrow');
2622 element.addEventListener('click', function(event) {
2623 event.stopPropagation();
2624 dropdownMenu.classList.toggle('show');
2625 if (dropdownMenu.classList.contains('show')) {
2626
document.querySelector('.show-lang-pc').style="left:"+(event.target.closest('.dropdown-btn').getBoundingCli
entRect().right+
window.scrollX)+"px;"+"top:"+(event.target.closest('.dropdown-btn').getBoundingClientRect().top+
window.scrollY)+"px"
2627 arrow.classList.add('fa-chevron-down');
2628 arrow.classList.remove('fa-chevron-up');
2629 } else {
2630 document.querySelector('.show-lang-pc').removeAttribute('style');
2631 arrow.classList.remove('fa-chevron-down');
2632 arrow.classList.add('fa-chevron-up');
2633 }
2634 });
2635 });
